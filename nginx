server {
    listen 80;
    server_name 0.0.0.0;

    location = /favicon.ico { access_log off; log_not_found off; }

    location /static/ {
            alias /home/ubuntu/election/static;
    }

    location /media/ {
           alias /home/ubuntu/election/static;
   }

    location / {
            include proxy_params;
            proxy_pass http://unix:/home/ubuntu/election/election.sock;
    }
}
